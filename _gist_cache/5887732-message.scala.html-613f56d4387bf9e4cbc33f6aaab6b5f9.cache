@(title: String, msg: String)
@main(title){<script src="@routes.Assets.at("javascripts/message.js")" type="text/javascript"></script>}{
<div class="group">
    <article role="article" class="group">
        <section class="content-group">
            <div class="group">
                <p>@msg</p>
            </div>
        </section>
    </article>
</div>
}